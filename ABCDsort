
public class TEST05ABCD {//ABCD 排列組合 要"由小到大" 排列

	public static void main(String[] args) {
		String s[] = {"A", "B", "C", "D"};
		String result = "";
		for(int i = s.length; i > 0; i--) {
			for(int j = s.length-i; j < s.length; j++) {
				result = result + s[j];
				System.out.println(result);
			}
			result = "";
		}
	    
	}

}
